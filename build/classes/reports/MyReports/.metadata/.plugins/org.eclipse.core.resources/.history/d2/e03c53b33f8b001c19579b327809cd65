	<queryString>
		<![CDATA[select p.idpes, p.nome, p.cpf, e.nome, e.cnpj, DATE_FORMAT (i.datainteg,'%d/%m/%Y'), DATE_FORMAT (i.validade,'%d/%m/%Y'), i.statusInteg, i.setor from integracao i
join pessoas p
on i.idpes = p.idpes
join empresas e
on e.idemp = i.idemp
where i.statusInteg='1'
order by e.nome]]>
	</queryString>
