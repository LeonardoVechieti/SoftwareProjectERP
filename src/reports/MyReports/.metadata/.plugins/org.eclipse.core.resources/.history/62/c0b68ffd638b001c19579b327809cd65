	<queryString>
		<![CDATA[SELECT idpes,
	nome,
	cpf,
	nome AS empresa,
	cnpj,
	DATE_FORMAT ( i.datainteg,'%d/%m/%Y') AS datainteg,
	DATE_FORMAT ( i.validade,'%d/%m/%Y') AS datavalidade,
	statusInteg,
	setor
FROM integracao i
	join pessoas p ON 
	 'NULL' = 'NULL' 
	join empresas e ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' = '1' 
ORDER BY nome DESC]]>
	</queryString>
